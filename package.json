{
  "name": "1mcloud_requests",
  "version": "1.0.0",
  "description": "1mcloud manager, proxy checker",
  "main": "./src/server/server.js",
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/style.css --minify",
    "dev:css": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/style.css --watch",
    "dev:server": "nodemon ./src/server.js",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:server\"",
    "debug": "node --inspect ./src/server.js",
    "build": "tailwindcss -i ./src/public/css/input.css -o ./src/public/css/style.css --minify",
    "start": "node ./src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hq1anq/1mcloud_tools.git"
  },
  "author": "Trinh Hoang Giang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hq1anq/1mcloud_tools/issues"
  },
  "homepage": "https://github.com/Hq1anq/1mcloud_tools#readme",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "dotenv": "^17.1.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "https-proxy-agent": "^7.0.6",
    "mysql2": "^3.14.2",
    "node-fetch": "^3.3.2",
    "socks-proxy-agent": "^8.0.5",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  }
}
